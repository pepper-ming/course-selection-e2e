{
  "name": "course-selection-e2e",
  "version": "1.0.0",
  "description": "課程選課系統 E2E 自動化測試",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:auth": "playwright test tests/auth.spec.js",
    "test:course": "playwright test tests/course-query.spec.js",
    "test:enrollment": "playwright test tests/enrollment.spec.js",
    "test:mycourses": "playwright test tests/my-courses.spec.js",
    "test:full": "playwright test tests/auth.spec.js tests/course-query.spec.js tests/enrollment.spec.js tests/my-courses.spec.js --reporter=list",
    "setup": "node scripts/setup-test-data.js",
    "cleanup": "node scripts/cleanup-test-data.js",
    "install:browsers": "playwright install",
    "postinstall": "playwright install"
  },
  "dependencies": {
    "@playwright/test": "^1.53.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "course-selection",
    "automation"
  ],
  "author": "",
  "license": "MIT"
}